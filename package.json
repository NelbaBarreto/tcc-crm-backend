{
  "name": "tcc-crm-backend",
  "version": "1.0.0",
  "description": "Backend sistema de gesti√≥n de relacionamiento con clientes",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"No test specified\"",
    "dev": "nodemon server.js --exec babel-node index.js",
    "prepare": "husky install",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo",
    "db:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx eslint --fix . --ext .js"
    ]
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "postgresql",
    "rest",
    "api"
  ],
  "author": "nbarreto",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "eslint": "^8.18.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16"
  }
}
